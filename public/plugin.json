{
    "pluginName": "Code计算器",
    "description": "rcalculator",
    "author": "Ribs",
    "main": "index.html",
    "preload": "preload.js",
    "logo": "logo6.png",
    "version": "0.2.2",
    "features": [
        {
            "code": "rcalculator",
            "explain": "Code计算器",
            "cmds": [
                "rcalculator",
                "Code计算器",
                {
                    "type": "regex",
                    "label": "Code计算器",
                    "match": "/^[\\.+\\-*/%0-9\\(\\)]*$/",
                    "minLength": 1
                },
                {
                    "type": "regex",
                    "label": "Code计算器",
                    "match": "/^=/",
                    "minLength": 2
                }
            ]
        }
    ],
    "pluginSetting": {
        "height": 450
    }
}